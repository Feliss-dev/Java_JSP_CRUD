CREATE TABLE MayTinh (
    MaMayTinh INT PRIMARY KEY AUTO_INCREMENT,
    TenMayTinh VARCHAR(50),
    HangSanXuat VARCHAR(50),
    SoLuong INT,
    DonGia DECIMAL(10, 2)
);

